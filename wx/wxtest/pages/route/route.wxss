.h{
  display: flex;
  flex-direction: row
}
.v{
  display: flex;
  flex-direction: column
}
.width-full{
  width: 100%
}
.height-full{
  height: 100%
}
.top{
  background-color: #fff;
  height: 80rpx;
  border-bottom: 2px solid #f0f0f0
}
.tabs{
  width: 88%
}
.v90{
  height: 70rpx;
  width: 2px;
  top: 5rpx;
  background-color: #f0f0f0
}
.search{
  width: 60rpx;
  height:60rpx;
  margin-top: 5rpx;
  margin-left: 5rpx;
}
.content{
  padding: 0;
}

.container{
  border: 2rpx black solid;
  width: 400rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.item{
	width:100%;height:280rpx;border-top:8px solid #f0f0f0;background-color:#fff;border-bottom:1px solid #f0f0f0;
}
.itemIcon{
	width:100rpx;height:100rpx;margin-top:25rpx;margin-left:25rpx;margin-right:25rpx;
}
.itemOrder{
	font-size:26rpx;margin-top:40rpx;height:40rpx;
}
.itemProcessView{
	width:100%;margin-top:20rpx;
}
.itemAddressView{
	align-items: center;margin-top:5rpx;margin-bottom:5rpx;
}