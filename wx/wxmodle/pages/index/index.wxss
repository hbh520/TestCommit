page,.wuss-weapp-example  {
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}

.header {
  padding: 20px 0;
  display: flex;
  flex: 1;
  flex-direction: column;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}

.logo {
  width: 100px;
  height: 100px;
  /* background-color: #eeeeee; */
}

.title {
  padding: 15px;
  font-size: 24px;
  background-image: linear-gradient(92deg, rgba(255, 102, 0,1), rgba(255, 153, 51,1),rgba(204, 0, 51,1));
  background-image: -webkit-linear-gradient(92deg,  rgba(255, 102, 0,1), rgba(255, 153, 51,1),rgba(204, 0, 51,1));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  -webkit-animation: filter-hue 30s infinite linear;
}

@-webkit-keyframes filter-hue {
  from {
    -webkit-filter: hue-rotate(0deg);
  }

  to {
    -webkit-filter: hue-rotate(-360deg);
  }
}


