<view
 class="wuss-weapp-example"
 hover-class="none"
 hover-stop-propagation="false"
>
  <view class="header">
    <image class="logo" src="/assets/logo.png" ></image>
    <text class="title">Wuss Weapp</text>
  </view>

  <view class="example-content">
    <w-pane desc="基础组件" wuss-class="w-pane" />
    <w-cell-group>
      <w-cell highlight label="Button 按钮" link="../w-button/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Icon 图标" link="../w-icon/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Avatar 头像" link="../w-avatar/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Steps 步骤条" link="../w-steps/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Countdown 倒计时" link="../w-countdown/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Dialog 消息框" link="../w-dialog/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Badge 徽章" link="../w-badge/index" wuss-class="w-cell" isLink/>
      <w-cell highlight label="Tag 标签" link="../w-tag/index" wuss-class="w-cell" isLink/>
    </w-cell-group>


    <w-pane desc="布局" wuss-class="w-pane" />
    <w-cell-group>
      <w-cell highlight label="Pane 窗格" link="../w-pane/index" wuss-class="w-cell" isLink />
    </w-cell-group>


    <w-pane desc="导航" wuss-class="w-pane" />
    <w-cell-group>
      <w-cell highlight label="Cell 列表" link="../w-cell/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Popup 弹出层" link="../w-popup/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="DropMenu 下拉菜单" link="../w-drop-menu/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Tabs 导航切换栏" link="../w-tabs/index" wuss-class="w-cell" isLink />
    </w-cell-group>


    <w-pane desc="操作反馈" wuss-class="w-pane" />
    <w-cell-group>
      <w-cell highlight label="ActionSheet 操作菜单" link="../w-action-sheet/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Toast 提示" link="../w-toast/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Alert 弹出框" link="../w-alert/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Notice 通告" link="../w-notice/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="ActivityIndicator 活动指示器" link="../w-activity-indicator/index" wuss-class="w-cell" isLink />
    </w-cell-group>


    <w-pane desc="表单" wuss-class="w-pane" />
    <w-cell-group >
      <w-cell highlight label="Form 表单" link="../w-form/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Input 输入框" link="../w-input/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Switch 开关" link="../w-switch/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Radio 单选框" link="../w-radio/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="Checkbox 复选框" link="../w-checkbox/index" wuss-class="w-cell" isLink />
    </w-cell-group>
    

    <w-pane desc="其他" wuss-class="w-pane" />
    <w-cell-group>
      <w-cell highlight label="Calendar 日历预定选择器" link="../w-calendar/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="QRCode 二维码生成" link="../w-qr-code/index" wuss-class="w-cell" isLink />
      <w-cell highlight label="SwiperOut 侧滑菜单" link="../w-swiper-out/index" wuss-class="w-cell" isLink />
    </w-cell-group>

  </view>
</view>

